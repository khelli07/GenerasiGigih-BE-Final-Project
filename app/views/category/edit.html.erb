<div class="container" style="margin-top:13%; width:50%">
  <h2 class="mb-3">Editing Kategori <%= @category.name %></h2>
  
  <%= form_with(model: @category, url: category_path) do |form| %>
  <div class="form-row mb-3">
    <div class="col">
    <%= form.text_field :name, class: "form-control", placeholder: @category.name %>
    </div>
  <input class="btn btn-success" type="submit" value="Selesai">
  <% end %>
</div>